{
  "name": "server",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "copy-deps": "npm run copy-node-modules && npm run copy-tickers",
    "copy-node-modules": "copy-newer --cwd node_modules ** ../dist/node_modules",
    "copy-tickers": "copy-newer --cwd ./src/services tickers.json ../dist/server/services/",
    "start": "node ../dist/server/server.js",
    "preserve": "npm run build",
    "s": "npm run serve",
    "serve": "concurrently \"npm run build.w\" \"nodemon --watch ../dist/server ../dist/server/server.js\"",
    "lint": "tslint \"src/**/*.ts\"",
    "prebuild": "npm install && rimraf ../dist/public",
    "build": "npm run copy-deps && tsc",
    "b": "npm run build.w",
    "build.w": "npm run copy-deps && tsc -w"
  },
  "private": true,
  "dependencies": {
    "body-parser": "1.17.1",
    "cors": "2.8.3",
    "dateformat": "2.0.0",
    "express": "4.15.3",
    "inversify": "4.1.0",
    "inversify-express-utils": "3.5.1",
    "morgan": "1.8.1",
    "reflect-metadata": "0.1.10",
    "request": "2.81.0",
    "request-promise": "4.2.1",
    "ws": "^3.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.3",
    "@types/cors": "2.8.1",
    "@types/dateformat": "1.0.1",
    "@types/express": "4.0.35",
    "@types/googlemaps": "^3.26.10",
    "@types/morgan": "1.7.32",
    "@types/node": "7.0.14",
    "@types/request-promise": "4.1.33",
    "@types/ws": "0.0.41",
    "concurrently": "3.4.0",
    "copy-newer": "^2.1.2",
    "morgan": "1.8.1",
    "nodemon": "1.11.0",
    "rimraf": "2.6.1",
    "tslint": "5.2.0",
    "typescript": "2.3.2"
  }
}
